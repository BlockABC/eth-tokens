<script>
	$(document).ready(function() {
		$("#newsletter .form-group label.valid").hide();
		function validateEmail($email) {
 			var emailReg = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*(\+[a-z0-9-]+)?@[a-z0-9-]+(\.[a-z0-9-]+)*$/;
 			return emailReg.test( $email );
		}
		$("#newsletter a#signUpNewsletter").on( "click", function(event) {
			event.preventDefault();
			var email = document.getElementById("newsletterInput").value;
			if( !validateEmail(email) || email=="") {
				$("#newsletter .form-group label.valid").show();
				return;
			} else {
				jQuery.ajax({
					type: 'POST',
					url: "/add",
					headers: {
						'X-CSRF-Token': 'Dh5pSnjElG1tDFHnlCA7CsxCYoilLPs2pisxHbIV5HOGCK5nmOmrP9zSSprammM5Wcf1OdLzucCQvz7Lx4g+tA=='
					},
					data: {email: email},
					dataType: 'json',
					success: function(data) {
						if (data.success) {
							$("#newsletter .form").hide();
							$("#newsletter .thanks").show();
			                $(".economy .form").hide();
			                $(".economy .thanks").show();
						} else {
							$("#newsletter .form-group label.valid").text(email + " looks invalid, please enter a real email address.")
							$("#newsletter .form-group label.valid").show();
						}
					}, error: function (err) {
							var r = jQuery.parseJSON(err.responseText);
							r.msg = r.msg.replace("Use PUT to insert or update list members.", "");
							$(".form-group label.valid").text(r.msg)
							$(".form-group label.valid").show();
          }
				});
			}
		});
	})
</script>

