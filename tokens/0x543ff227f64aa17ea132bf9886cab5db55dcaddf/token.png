		function validateEmail($email) {
			var emailReg = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*(\+[a-z0-9-]+)?@[a-z0-9-]+(\.[a-z0-9-]+)*$/;
			return emailReg.test( $email );
		}
		$(".economy a#signUp").on( "click", function(event) {
			event.preventDefault();
			var email = document.getElementById("tokenSale").value;
			if( !validateEmail(email) || email=="") {
				$(".form-group label.valid").show();
				return;
			} else {
				jQuery.ajax({
					type: 'POST',
					url: "/add",
					headers: {
						'X-CSRF-Token': 'MIYHKJ20T80CLOeupLh/doW7rNBVzbGo0MPOzqgKUaG4kMAFfZlwn7Py/NPqAidFED47YSIS817mV8EY3ZeLZg=='
					},
					data: {email: email},
					dataType: 'json',
					success: function(data) {
						if (data.success) {
							$(".form").hide();
							$(".thanks").show();
						} else {
							$(".form-group label.valid").text(data.msg)
							$(".form-group label.valid").show();
